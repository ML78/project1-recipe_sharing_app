<h1>Sign up</h1>

<!-- error handling -->
<div>
  <% if @user.errors.any? %>
    <ol>
      <% @user.errors.full_messages.each do |msg| %>
        <li class="alert alert-danger" role="alert"><%= msg.capitalize %></li>
      <% end %>
    </ol>
  <% end %>
</div>

<div>
  <%= render :partial => 'form' %>
</div>
